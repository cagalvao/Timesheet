{
  "name": "mono_api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clear_docker_volume": "find ../infra/docker_volumes/mono_api/* ! -name . -prune ! -name node_modules -prune ! -name coverage -type d -or ! -name README.md ! -name wait-for-it.sh -type f | xargs rm -rf",
    "update_docker_volume": "rsync -avzl --exclude=node_modules ./ ../infra/docker_volumes/mono_api/",
    "refresh_docker_volume": "npm run clear_docker_volume && npm run update_docker_volume",
    "start": "pm2 start index.js --no-daemon --watch --node-args=\"--inspect=0.0.0.0:9229\"",
    "start_mono_api": "npm run refresh_docker_volume && docker-compose up mono_api",
    "start_mono_api_silent": "npm run refresh_docker_volume && docker-compose up -d mono_api",
    "static_test": "standard",
    "clear_swagger": "find ../infra/docker_volumes/swagger/* ! -name README.md -type f | tac | xargs rm -rf",
    "update_swagger": "rsync swagger/doc/* ../infra/docker_volumes/swagger/",
    "swagger": "npm run clear_swagger && node swagger/swagger.js && npm run update_swagger"
  },
  "author": "Cássio Galvão <cassio.galvao@outlook.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "chai": "^1.10.0",
    "chai-sorted": "^0.1.0",
    "eslint": "^3.18.0",
    "istanbul": "^0.4.5",
    "json-refs": "^2.1.7",
    "libxmljs": "^0.18.7",
    "mocha": "^3.2.0",
    "mock-knex": "^0.3.8",
    "mockdate": "^2.0.1",
    "njwt": "^0.4.0",
    "onchange": "^3.2.1",
    "proxyquire": "^1.8.0",
    "rewire": "^2.5.2",
    "should": "^11.2.1",
    "should-sinon": "0.0.5",
    "sinon": "^2.1.0",
    "sqlite3": "^3.1.13",
    "standard": "10.0.3",
    "supertest": "^0.15.0"
  },
  "dependencies": {
    "appdynamics": "4.3.7",
    "async": "^2.2.0",
    "aws-sdk": "^2.49.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "cacheman": "^2.2.1",
    "cookie-parser": "^1.4.3",
    "elasticsearch": "^13.0.0",
    "express": "^4.15.2",
    "express-validation": "^1.0.2",
    "helmet": "^3.11.0",
    "http-aws-es": "git+https://github.com/rmsphd/http-aws-es.git",
    "ioredis": "^3.2.1",
    "joi": "^10.5.2",
    "jsonwebtoken": "^7.3.0",
    "jwt-simple": "^0.5.1",
    "knex": "^0.12.9",
    "ldapjs": "^1.0.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.14",
    "morgan": "^1.8.1",
    "morgan-json": "^1.1.0",
    "mysql": "^2.15.0",
    "oracledb": "^1.12.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pm2": "^2.4.6",
    "prompt": "^1.0.0",
    "randomstring": "^1.1.5",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "unidecode": "^0.1.8",
    "uuid": "^3.0.1",
    "winston": "^2.3.1",
    "xml2js": "^0.4.17"
  },
  "standard": {
